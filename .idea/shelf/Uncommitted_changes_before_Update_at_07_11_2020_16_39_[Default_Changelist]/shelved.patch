<+>{% extends 'base.html.twig' %}\r\n\r\n{% block title %}Connexion{% endblock %}\r\n\r\n\r\n\r\n{% block body %}\r\n    <body>\r\n    <h5><a class=\"p-2 text-dark link-login\" href=\"{{ path('app_register') }}\">S'inscrire</a></h5>\r\n\r\n    <form class=\"form-login\" method=\"post\">\r\n        {% if error is defined %}\r\n            {% if error %}\r\n                <div class=\"alert alert-danger\">{{ error.messageKey|trans(error.messageData, 'security') }}</div>\r\n            {% endif %}\r\n        {% endif %}\r\n\r\n\r\n        {% if app.user %}\r\n            <div class=\"mb-3\">\r\n                You are logged in as {{ app.user.username }}, <a href=\"{{ path('app_logout') }}\">Logout</a>\r\n            </div>\r\n        {% endif %}\r\n\r\n        <h1 class=\"h3 mb-3 font-weight-normal\">Connectez-vous</h1>\r\n        <label for=\"inputEmail\">Email</label>\r\n        <input type=\"email\" value=\"{{ last_username }}\" name=\"email\" id=\"inputEmail\" class=\"form-control\" required\r\n               autofocus>\r\n        <label for=\"inputPassword\">Password</label>\r\n        <input type=\"password\" name=\"password\" id=\"inputPassword\" class=\"form-control\" required>\r\n\r\n        <input type=\"hidden\" name=\"_csrf_token\"\r\n               value=\"{{ csrf_token('authenticate') }}\">\r\n\r\n        <div class=\"checkbox mb-3\">\r\n            <label>\r\n                <input type=\"checkbox\" name=\"_remember_me\"> Se souvenir de moi\r\n            </label>\r\n\r\n            <a class=\"p-2 text-dark\" href=\"#\">Mot de passe oublié</a>\r\n\r\n        </div>\r\n        <button class=\"btn btn-lg btn-primary\" type=\"submit\">\r\n            Connexion\r\n        </button>\r\n    </form>\r\n    </body>\r\n{% endblock %}\r\n
